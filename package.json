{
  "name": "air-invoice-promo",
  "version": "1.0.0",
  "private": true,
  "description": "Air Invoice Promotion Site",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "build": "run-s clean build:prod build:local",
    "build:local": "gatsby build",
    "build:prod": "run-s build:prod:*",
    "build:prod:pre": "npm run rmHtdocsInvoice",
    "build:prod:build": "gatsby build --prefix-paths",
    "build:prod:post": "run-s cpPublic rmPublic",
    "rmHtdocsInvoice": "rm -rf htdocs/invoice/**",
    "cpPublic": "cp -r public/ htdocs/invoice/",
    "rmPublic": "rm -r public",
    "serve": "gatsby serve",
    "clean": "run-s cleanonly rmHtdocsInvoice",
    "cleanonly": "gatsby clean",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx \"src/**/*.{js,ts,jsx,tsx}\"",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "prettier": "prettier \"*.{json,yml,yaml,md,js}\" \"src/**/*.{js,ts,jsx,tsx,yml,yaml,json,md,css,scss}\"",
    "fix": "run-p fix:*",
    "fix:eslint": "npm run eslint -- --fix",
    "fix:stylelint": "npm run stylelint -- --fix",
    "fix:prettier": "npm run prettier -- --write",
    "lint": "run-p lint:*",
    "lint:typecheck": "tsc",
    "lint:eslint": "npm run eslint",
    "lint:stylelint": "npm run stylelint",
    "lint:format": "npm run prettier -- --check",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s ./src",
    "buildStorybook": "build-storybook",
    "icons": "npx @svgr/cli -d src/components/atoms/Icon/ src/images/icon/"
  },
  "dependencies": {
    "@types/gatsby-plugin-breakpoints": "1.3.2",
    "@types/react-burger-menu": "^2.8.6",
    "@types/react-helmet": "6.1.4",
    "babel-plugin-styled-components": "2.0.2",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "gatsby": "4.3.0",
    "gatsby-link": "4.3.0",
    "gatsby-plugin-breakpoints": "1.3.6",
    "gatsby-plugin-image": "2.5.1",
    "gatsby-plugin-manifest": "4.6.0",
    "gatsby-plugin-no-sourcemaps": "4.12.1",
    "gatsby-plugin-react-helmet": "5.3.0",
    "gatsby-plugin-root-import": "2.0.8",
    "gatsby-plugin-sass": "5.5.0",
    "gatsby-plugin-sharp": "4.5.1",
    "gatsby-plugin-sitemap": "5.11.1",
    "gatsby-plugin-styled-components": "5.5.0",
    "gatsby-plugin-typegen": "2.2.4",
    "gatsby-plugin-use-query-params": "1.0.1",
    "gatsby-react-router-scroll": "5.3.0",
    "gatsby-source-filesystem": "4.5.1",
    "react": "17.0.1",
    "react-burger-menu": "^3.0.9",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "sass": "1.49.0",
    "scroll-behavior-polyfill": "2.0.13",
    "styled-components": "5.3.3",
    "use-query-params": "1.2.3"
  },
  "devDependencies": {
    "@babel/core": "7.17.2",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/addon-interactions": "6.4.19",
    "@storybook/addon-links": "6.4.19",
    "@storybook/builder-webpack5": "6.4.19",
    "@storybook/manager-webpack5": "6.4.19",
    "@storybook/react": "6.4.19",
    "@storybook/testing-library": "0.0.9",
    "@svgr/cli": "6.2.1",
    "@types/styled-components": "5.1.17",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "eslint": "8.4.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-storybook": "0.5.6",
    "husky": "7.0.4",
    "lint-staged": "12.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "storybook-addon-gatsby": "0.0.5",
    "stylelint": "14.1.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "24.0.0",
    "stylelint-order": "5.0.0",
    "typescript": "4.5.2"
  }
}
